package OopsConceptPart1;

public class FunctionsInJava {

	public static void main(String[] args) {
		

	}
	//non static methods
	
	//void -- doesnot return any value
	public void test() {
		System.out.println("test method");
	}
    
	public int pqr() {
		System.out.println("pqr method");
		int a = 10;
		int b = 20;
		int c = a+b;
		return c;
	}
}
