package myproject;

public class conditionalOperator13 {

	public static void main(String[] args) {
		/* Types of Conditions
		 == equals to
		 != not equals to
		 >  greater than
		 >= greater than equals to
		 <  less than
		 <= less than equals to		
		*/		
		int age = 28;
		System.out.println(age > 50 ? "You are old" : "You are young");
		System.out.println(age == 30 ? "You are not equal to 30" : "You are equal to 28");
		
		int age1 = 52;
		System.out.println(age1 > 50 ? "You are greater than 50" : "You are not greater than 50");
		System.out.println(age1 >= 50 ? "You are not greater than equals to 50" : "You are greater than equals to 50");

	}

}
