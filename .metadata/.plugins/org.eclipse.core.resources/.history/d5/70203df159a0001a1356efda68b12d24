package myproject;

public class calc {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int x, y, answer;
		x = 20;
		y = 30;
		answer = x+y;
		System.out.println("Answer is " + answer);
		
		x = 20;
		y = 30;
		answer = y-x;
		System.out.println("Answer is " + answer);
		
		x = 20;
		y = 30;
		answer = x*y;
		System.out.println("Answer is " + answer);



	}

}
