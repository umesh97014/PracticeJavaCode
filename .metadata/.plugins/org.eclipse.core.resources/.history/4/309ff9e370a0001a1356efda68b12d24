package myproject;

public class forLoop18 {

	public static void main(String[] args) {
		// For Loop
		for (int counter = 0; counter < 10; counter++) {
			System.out.println(counter);
		}
		System.out.println("==================================");
		
		for (int counter = 2; counter <= 10; counter+=2) {
			System.out.println(counter);
		}

	}

}
