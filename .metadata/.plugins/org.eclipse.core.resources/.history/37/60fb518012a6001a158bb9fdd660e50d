package practiceJAVA;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class salesforceE2E37 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.setProperty("webdriver.chrome.driver", "C:\\Users\\umekumar2\\eclipse-workspace\\automationpractice\\src\\main\\resources\\chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		driver.get("https://rediff.com");//hit url on the browser
		driver.findElement(By.cssSelector("a[title*='Already']")).click();
		driver.findElement(By.xpath("//input[@id='login1']")).sendKeys("umesh97014@rediffmail.com");
		driver.findElement(By.xpath("//input[@id='password']")).sendKeys("Sim14sim");
		
	}

}
